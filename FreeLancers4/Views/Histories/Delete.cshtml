@model FreeLancers4.Models.History

@{
    ViewData["Title"] = "Delete";
    ViewData["name"] = "hitory-delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="general-details-container">
    <div class="general-details-header">Details</div>
    <div class="general-details-subheading">Confirm details below</div>
    <div class="general-details-content-div">

        <div class="general-details-content">
            <div class="general-details-content-left">Title</div>
            <div class="general-details-content-right">@Html.DisplayFor(model => model.Proffesion)</div>
        </div>

        <div class="general-details-content">
            <div class="general-details-content-left">Posted Date</div>
            <div class="general-details-content-right">@Html.DisplayFor(model => model.completeDate)</div>
        </div>

        <div class="general-details-content">
            <div class="general-details-content-left">Description</div>
            <div class="general-details-content-right">@Html.DisplayFor(model => model.Project)</div>
        </div>

    </div>

    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <div class="client-work-card-section-2-bottom-button">
            <a href="#"><button type="submit">Delete</button></a>
        </div>
        <a asp-action="Index">
            <div class="forgot-password">
                Back to List
            </div>
        </a>
    </form>
</div>
